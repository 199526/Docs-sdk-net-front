{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/project.json"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/*.Tests/project.json"
          ],
          "cwd": "../azure-sdk-for-net/src/ResourceManagement"
        }
      ],
      "dest": "api_ref"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "*.yml"
        ],
        "src": "api_ref",
        "dest": "."
      },
      {
        "files": [
          "*.md"
        ],
        "exclude": [
          "api-doc/**/*.md"
        ],
        "src": "api",
        "dest": "."
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "toc",
        "dest": "toc"
      }
    ],
    "overwrite": [
      {
        "files": [
          "api-doc/**/*.md"
        ]
      }
    ],
    "globalMetadata": {
      "breadcrumb_path": "~/toc/toc.yml"
    },
    "dest": "_site",
    "template": [
      "docs.html"
    ]
  }
}