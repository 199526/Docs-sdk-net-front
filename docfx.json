{
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml",
          "**/*.md"
        ],
        "exclude": [
          "**/toc.yml",
          "overwrites/**/*"
        ],
        "src": "api",
        "dest": "api"
      },
      {
        "files": [
          "**/toc.yml"
        ],
        "src": "api",
        "dest": "api/azure_ref_toc"
      },
      {
        "files": [
          "index.md"
        ]
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "breadcrumb",
        "dest": "azure_breadcrumb"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-conceptual",
        "dest": "azure"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "docs-ref-overwrite/**/*.md",
          "api/overwrites/**/*.md"
        ]
      }
    ],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "Azure/azure-docs-sdk-dotnet",
      "feedback_product_url": "https://feedback.azure.com/forums/170031-sdk-and-tools",
      "author": "CamSoper,
      "ms.author": "casoper",
      "ms.topic": "reference",
      "ms.devlang": "csharp",
      "ms.date": "11/16/2016",
      "ms.prod": "azure-dotnet",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "brand": "azure",
      "apiPlatform": "dotnet",
      "_op_documentIdPathDepotMapping": {
        "./": {
          "folder_relative_path_in_docset": "api-index"
        },
        "breadcrumb/": {
          "folder_relative_path_in_docset": "breadcrumb"
        },
        "docs-ref-overwrite/": {
          "folder_relative_path_in_docset": "docs-ref-overwrite"
        },
        "scripts/": {
          "folder_relative_path_in_docset": "scripts"
        }
      }
    },
    "fileMetadata": {
      "feedback_system": 
      {
             "xml/**/*": "None"
      },
      "version": {
        "api/overview/azure/**/*.yml": ["azure-dotnet"],
        "api/overview/azure/**/*.md": ["azure-dotnet"],
        "docs-ref-conceptual/*.md": ["azure-dotnet"]
      },
      "monikers": {
        "api/overview/azure/**/*.yml": ["azure-dotnet"],
        "api/overview/azure/**/*.md": ["azure-dotnet"],
        "docs-ref-conceptual/*.md": ["azure-dotnet"]
      },
      "langs": {
        "api/**.yml": ["csharp", "vb", "fsharp"]
      }.
      "ms.topic": {
        "api/**.yml": "managed-reference"
      }
    },
    "markdownEngineName": "markdig",
    "dest": "_site",
    "template": [
      "docs.html"
    ],
    "lruSize": 0,
    "debug": true
  }
}
